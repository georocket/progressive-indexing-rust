{
    "version": "2.0.0",
    "tasks": [
      {
        "label": "build",
        "type": "shell",
        "command": "cargo build",
        "problemMatcher": ["$rustc"],
        "group": {
          "kind": "build",
          "isDefault": true
        }
      },
      {
        "label": "build_release",
        "type": "shell",
        "command": "cargo build --release",
        "problemMatcher": ["$rustc"],
        "group": {
          "kind": "build",
          "isDefault": true
        }
      }
    ]
  }
  